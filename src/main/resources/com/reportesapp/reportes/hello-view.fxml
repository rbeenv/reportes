<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="235.0" prefWidth="242.0" spacing="10" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.reportesapp.reportes.ControladorMain">
    <padding>
        <Insets bottom="20" left="20" right="20" top="20" />
    </padding>
    <ImageView fx:id="logoImage" fitHeight="100" fitWidth="202.0">
        <image>
            <Image url="@../../../images/imagenLogoMensajeria.png" />
        </image>
    </ImageView>
    <VBox alignment="CENTER" spacing="20" style="-fx-padding: 50 0 0 0;">
        <Button fx:id="btnReportePedido" onAction="#pedidos" text="Crear Reporte de Pedido" />
        <Button fx:id="btnReporteClientes" onAction="#clientes" text="Crear Reporte de Clientes" />
    </VBox>
</VBox>
